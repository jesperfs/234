#include <stdio.h>

void assign(static int* num1)
{
	*num1 = 4;
	return;
}

int main(void)
{
	static int num1 = -3;            // Uppgift A
	printf("%d\n", num1);            // Uppgift B
	int* ptr1 = &num1;               // Uppgift C
	assign(&num1);                   // Uppgift D
	printf("%d\n", num1);            // Uppgift E

	return 0;
}
